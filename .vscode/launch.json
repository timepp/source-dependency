    "version": "0.2.0",
    "configurations": [


        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/source-dependency.ts",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "args": [ "../px_web", "--language=javascript", "--exclude-path=node_modules" ],
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        }
    ]
}