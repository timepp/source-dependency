{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "request": "launch",
            "name": "Launch Program",
            "type": "node",
            "program": "${workspaceFolder}/sd.ts",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "C:\\Users\\junhuitong\\.deno\\bin\\deno.EXE",
            "runtimeArgs": [
                "run",
                "--unstable",
                "--inspect-wait",
                "--allow-all"
            ],
            "args": [
                "-l", "${input:language}",
                "-c", "${input:config}",
            ],
            "attachSimplePort": 9229
        }
    ],
    "inputs": [
        {
            "id": "language",
            "type": "pickString",
            "description": "language",
            "options": ["typescript","C++","all"],
            "default": "all"
        },
        {
            "id": "config",
            "type": "promptString",
            "description": "config",
            "default": ".jsonc"
        }
    ]
}